class Deportista():
    def __init__(self,nombre:str, documento:str, edad:int,Ganados: int, Perdidos:int, Equipo: str, Anotadas: int, Anotados:int):     #MÃ©todo Constructor
        self.__nombre=nombre
        self.__documento=documento
        self.__edad=edad
       # self.__ganados=Ganados
       # self.__perdidos=Perdidos
       # self.__equipo=Equipo
       # self.__anotadas=Anotadas
       # self.__anotados=Anotados

    #---------- Setters ------------
    def setNombre(self,nombre:str):
        self.__nombre=nombre

    def setCodigo(self,documento:int):
        self.__documento=documento
        
    def setCarrera(self,edad:int):
        self.__edad=edad
   # def setGanados(self, Ganados:int):
    #    self.__ganados=Ganados
        
    
    #---------- Getters ------------
    def getNombre(self):
        return self.__nombre
    
    def getCodigo(self):
        return self.__documento
    
    def getCarrera(self):
        return self.__edad
    
